{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "abis/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "data/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "lib/**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.js"
    },
    {
      "src": "/api/preview",
      "dest": "/api/preview.js"
    },
    {
      "src": "/abis/(.*)",
      "dest": "/abis/$1"
    },
    {
      "src": "/data/(.*)",
      "dest": "/data/$1"
    },
    {
      "src": "/map",
      "dest": "/map.html"
    },
    {
      "src": "/v1",
      "dest": "/index.html"
    },
    {
      "src": "/",
      "dest": "/index-v2.html"
    }
  ]
}
